<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://dbfound.googlecode.com/model" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dbfound.googlecode.com/model http://dbfound.googlecode.com/svn/tags/v2/model.xsd">

	<execute name="upload">
		<param name="path" dataType="varchar" />
		<param name="file_name" dataType="varchar" />
		<param name="file" dataType="file" />
		<sqls>
			<java className="com.nfwork.test.FileManager"/>
		</sqls>
	</execute>
	
	<execute name="listFile">
		<param name="path" dataType="varchar" />
		<sqls>
			<java className="com.nfwork.test.FileManager" method="listFile"/>
		</sqls>
	</execute>
	
	<execute name="delete">
		<param name="path" dataType="varchar" scope="param" />
		<param name="file_name" dataType="varchar" />
		<sqls>
			<java className="com.nfwork.test.FileManager" method="delete"/>
		</sqls>
	</execute>
	
	<execute name="down">
		<param name="path" dataType="varchar" sourcePath="param.parameters.path" />
		<param name="file_name" dataType="varchar" scope="param" />
		<param name="file" dataType="file" fileSaveType="disk" ioType="out"/>
		<sqls>
			<java className="com.nfwork.test.FileManager" method="down"/>
		</sqls>
	</execute>

</model>
