<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://dbfound.googlecode.com/model" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dbfound.googlecode.com/model http://dbfound.googlecode.com/svn/tags/v2/model.xsd">
	<query>
		<sql>
		 <![CDATA[
            select f.function_code, f.function_des, m.module_code
			  from sys_function f, sys_module m
			 where f.function_module = m.module_id
			 #AND_CLAUSE#
		 ]]>
		</sql>
		<filter name="function_code" express="f.function_code = ${@function_code}" />
	</query>
</model>